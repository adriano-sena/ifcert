{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=<DB_HOST>;Port=<DB_PORT>;Database=<DB_NAME>;Username=<DB_USER>;Password=<DB_PASSWORD>"
  },
  "Jwt": {
    "Issuer": "Ifcert.Api",
    "Audience": "Ifcert.Client",
    "Secret": "<JWT_KEY_MIN_32_CHARS>",
    "ExpirationMinutes": 60
  }
}
